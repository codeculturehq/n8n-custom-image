+// Converts SCREAMING_SNAKE_CASE to camelCase (e.g., MAX_TEAM_PROJECTS -> maxTeamProjects)
+const toCamelCase = (str: string): string => {
+	return str
+		.toLowerCase()
+		.replace(/_([a-z])/g, (_, letter) => letter.toUpperCase());
+};
+
+const parseQuotaOverrides = (): Map<string, number> => {
+	const quotas = new Map<string, number>();
+	for (const [key, value] of Object.entries(process.env)) {
+		if (key.startsWith('N8N_LICENSE_QUOTA_') && value) {
+			const rawName = key.replace('N8N_LICENSE_QUOTA_', '');
+			const quotaName = 'quota:' + toCamelCase(rawName);
