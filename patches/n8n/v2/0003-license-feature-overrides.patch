+// N8N_LICENSE_BLOCK_FEATURES: comma-separated list of features to force disable
+// N8N_LICENSE_FORCE_PLAN: force a specific plan name (Community, Team, Enterprise)
+const parseFeatureList = (envVar: string | undefined): Set<string> => {
+	if (!envVar) return new Set();
+	return new Set(envVar.split(',').map((f) => f.trim()).filter(Boolean));
+};
